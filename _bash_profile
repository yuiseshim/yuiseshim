# .bash_profile

if [ -f ~/.bashrc ] ; then
. ~/.bashrc
fi

# Setting Japanese Font
export LANG=ja_JP.UTF-8
export LESSCHARSET=utf-8

export PATH="$PATH:$HOME/.pyenv/shims"

## Set path for pyenv
export PYENV_ROOT="${HOME}/.pyenv"
if [ -d "${PYENV_ROOT}" ]; then
    export PATH=${PYENV_ROOT}/bin:$PATH
    eval "$(pyenv init -)"
    eval "$(pyenv virtualenv-init -)"
fi

#export CPLUS_INCLUDE_PATH=/Users/name/.pyenv/versions/anaconda-2.1.0/include/python2.7
export CPLUS_INCLUDE_PATH=/Users/yuiseshime/.pyenv/versions/2.7.12/include/python2.7

#jupyter notebook &

